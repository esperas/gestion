{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "ecole.gestion",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "ecole.gestion.view.App",
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ecole.gestion.i18n.i18n"
                }
            },
            "parents": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "suivi": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "users": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "params": {
                "type": "sap.ui.model.json.JSONModel"
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "ecole.gestion.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "slide"
            },
            "routes": [{
                "pattern": "",
                "name": "appHome",
                "target": "home"
                },
                {
                "pattern": "suivi",
                "name": "suivi",
                "target": "suivi"
                },
                {
                "pattern": "import",
                "name": "import",
                "target": "import"
                },
                {
                "pattern": "solde",
                "name": "solde",
                "target": "solde"
                },
                {
                "pattern": "detail/{id}",
                "name": "detail",
                "target": "detail"
                },
                {
                "pattern": "new/:id:",
                "name": "new",
                "target": "detail"
                }
                      ],
            "targets": {
                "home": {
                    "viewName": "Home",
                    "viewLevel" : 1
                },
                "suivi": {
                    "viewName": "Suivi",
                    "viewLevel" : 2
                },
                "import": {
                    "viewName": "Import",
                    "viewLevel" : 2
                },
                "solde": {
                    "viewName": "Solde",
                    "viewLevel" : 2
                },
                "detail": {
                    "viewName": "Detail",
                    "viewLevel" : 3
                }
         }
      }

    }
}
