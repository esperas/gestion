<mvc:View
          controllerName="ui5.template.controller.App"
          xmlns="sap.m"
          xmlns:mvc="sap.ui.core.mvc"
          displayBlock="true">


         <Text  text="{i18n>WELCOME_TITLE}" />
        <List id="Menu" headerText="{i18n>ListOfAllEmployees}" items="{menu>/Menu}">
            <items>
                <StandardListItem
                                  title="{menu>title}"
                                  iconDensityAware="false"
                                  iconInset="false"
                                  type="Navigation"
                                  press="onListItemPressed"/>
            </items>
        </List>





</mvc:View>
